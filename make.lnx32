# Make include file for FEBio on Linux 32bit

HOME_PATH = /home/sci/rawlins/Projects

PLAT = lnx32

CC = g++

# Add DPRINTHB for matrix output
# Compile with DNDEBUG for release version
DEF = -DLINUX -DPARDISO -DSVN

FLG = -static -O3 

# Pardiso solver
MKL_PATH = /opt/intel/mkllib/32/
MKL_LIB = -L$(MKL_PATH) -lmkl_lapack -lmkl_intel -liomp5 -lpthread

LIBS = $(MKL_LIB)

INC = -I..
